<?xml version="1.0" ?>
<joomfish type="contentelement">
  <name>Joomlaquiz - Quizzes</name>
  <author>JoomPlace</author>
  <version>3.7.6</version>
  <description>Definition for the JoomlaQuiz component</description>
  <reference>


	<table name="quiz_t_quiz">
  		<field type="referenceid" name="c_id" translate="0">ID</field>
  		<field type="titletext" name="c_title" translate="1">Title</field>
  		<field type="textarea" name="c_description" translate="1">Description</field>
		<field type="textarea" name="c_short_description" translate="1">Short Description</field>
		<field type="textarea" name="c_right_message" translate="1">Message when the answer is correct</field>
		<field type="textarea" name="c_wrong_message" translate="1">Message when the answer is incorrect</field>
		<field type="textarea" name="c_pass_message" translate="1">Message when user have passed quiz</field>
		<field type="textarea" name="c_unpass_message" translate="1">Message when user have failed quiz</field>
		<field type="textarea" name="c_metadescr" translate="1">Quiz Meta Description</field>
		<field type="textarea" name="c_keywords" translate="1">Quiz Meta Keywords</field>
		<field type="textarea" name="c_metatitle" translate="1">Quiz Meta Title</field>
  	</table>

  </reference>
</joomfish>